  "cpu": {
    "interval": 10,
    "format": "Û∞çõ {usage}%",
    "rotate": 0,
    "format-alt": "{icon0}{icon1}{icon2}{icon3}",
    "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"]
  },

  "memory": {
    "states": {
      "c": 90, // critical
      "h": 60, // high
      "m": 30 // medium
    },
    "interval": 30,
    "format": "Û∞æÜ {used}GB",
    "rotate": 0,
    "format-m": "Û∞æÖ {used}GB",
    "format-h": "Û∞ìÖ {used}GB",
    "format-c": "Ôê° {used}GB",
    "format-alt": "Û∞æÜ {percentage}%",
    "max-length": 10,
    "tooltip": true,
    "tooltip-format": "Û∞æÜ {percentage}%\nÓâ¶ {used:0.1f}GB/{total:0.1f}GB"
  },

  "custom/cpuinfo": {
    "exec": " ~/.config/hypr/scripts/cpuinfo.sh",
    "return-type": "json",
    "format": "{}",
    "rotate": 0,
    "interval": 5, // once every 5 seconds
    "tooltip": true,
    "max-length": 1000
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "rotate": 0,
    "format-icons": {
      "activated": "Û∞•î",
      "deactivated": "Ôê∫"
    }
  },

  "clock": {
    "format": "{:%I:%M %p}",
    "rotate": 0,
    "format-alt": "{:%R Û∞É≠ %d¬∑%m¬∑%y}",
    "tooltip-format": "<tt>{calendar}</tt>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "weekdays": "<span color='#aeaeae'><b>{}</b></span>",
        "today": "<span color='#ffffff'><b>{}</b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },

  "custom/pickcolor": {
    "format": "Ôóà{}",
    "rotate": 0,
    "exec": "echo ; echo Ôóà Pick a color to clipboard",
    "on-click": "hyprpicker -a; notify-send \"$(wl-paste)\" | wl-paste >> $HOME/shits/colorpicker.info",
    "interval": 86400, // once every day
    "tooltip": true
  },

  "hyprland/language": {
    "format": "{}",
    "format-en": "en",
    "format-ar": "ar",
    "exec": "echo ; echo Language",
    "on-click": "hyprctl switchxkblayout xremap next",
    "interval": 86400, // once every day
    "tooltip": true
