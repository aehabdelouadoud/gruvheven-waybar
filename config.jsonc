{
  "layer": "top",
  "position": "top",
  "width": 0,
  "height": 23,
  "spacing": 0,
  "margin-bottom": 0,
  "reload_style_on_change": true,

  "modules-left": [
    "custom/padd",
    "custom/todos",
    "hyprland/workspaces",
    "custom/padd"
  ],

  "modules-center": [
    "custom/padd",
    "mpris",
    "custom/l_end",
    "idle_inhibitor",
    "clock",
    "custom/r_end",
    "custom/padd"
  ],

  "modules-right": [
    "custom/padd",
    "custom/updates",

    "custom/l_end",
    "custom/pickcolor",
    "custom/r_end",

    "custom/l_end",
    "cpu",
    "memory",
